from django.db import models

class joblisting(models.Model):
    hiring_position_text=models.CharField("Which postion are you hiring for?",max_length=50)
    hiring_industry_text=models.CharField("Which industry do you belong to?" ,max_length=50)
    posting_publish_date=models.DateTimeField("date published")
    hiring_description_md=models.CharField("Please give a short description of the position you are hiring for:",max_length=2000)
    hiring_primary_location=models.CharField("Where would the applicant be expected to work at primarily?")
    posting_id=models.PostiveIntegerField("Job Posting ID:")
    
    
